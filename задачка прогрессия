public class gh {

    public static void main(String[] args) throws Exception {
        int i ;
        int a = 5;
        int b = a +(a + 5) / 2;
        int c = b + a;
        int d = c + b+a;
        int sum = 0;

        for ( i = 0; i < 1; i++) {
            sum = sum + d;

            System.out.println(sum);

        }
    }
//Напишите тут ваш код
}
