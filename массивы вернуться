public class ner {
    public ner() {}

         static ArrayList<ner> cats = new ArrayList<ner>();


    //add your code here - добавь свой код тут

    public static void main(String[] args) {



        for (int i = 0; i<10;i++){
            ner cat = new ner();
            cats.add(cat);

        }
        printCats();


    }

    public static void printCats() {
        for (int i=0;i<cats.size();i++) {
            System.out.println(cats.get(i));//add your step 3 code here - добавь свой код для пункта 3 тут
        }
    }
}
