public class Rectangle {
    private int name1;
    private int left, top, width, height;

    public void initialize(int left, int top, int width, int height) {
        this.name1 = left + top + width + height;
    }
    public void initialize(int left, int top) {
        this.name1 = left + top;}

    public void initialize(int left, int width, int height) {
        this.name1 = left + width + height;}

    public void initialize(Rectangle copy) {
        this.name1 = copy.left + copy.top + copy.width + copy.height;
    }
}
